    "@types/uuid": "^3.4.4",
    "assert-rejects": "^1.0.0",
    "bignumber.js": "^7.2.1",
    "codecov": "^3.0.4",
    "eslint": "^5.4.0",
    "eslint-plugin-node": "^8.0.0",
    "gts": "^0.8.0",
    "ink-docstrap": "git+https://github.com/docstrap/docstrap.git",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
