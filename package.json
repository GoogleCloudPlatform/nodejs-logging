    "google-auto-auth": "^0.10.1",
    "google-gax": "^0.16.1",
    "google-proto-files": "^0.16.0",
    "is": "^3.2.1",
    "lodash.merge": "^4.6.1",
    "protobufjs": "^6.8.6",
    "pumpify": "^1.5.1",
    "snakecase-keys": "^1.1.1",
    "stream-events": "^1.0.4",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@google-cloud/bigquery": "*",
    "@google-cloud/nodejs-repo-tools": "^2.3.0",
    "@google-cloud/pubsub": "*",
    "@google-cloud/storage": "*",
    "async": "^2.6.1",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "ink-docstrap": "git+https://github.com/docstrap/docstrap.git",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "methmeth": "^1.1.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "power-assert": "^1.6.0",
