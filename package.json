    "pumpify": "^1.5.1",
    "snakecase-keys": "^2.0.0",
    "stream-events": "^1.0.4",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@google-cloud/bigquery": "^2.0.1",
